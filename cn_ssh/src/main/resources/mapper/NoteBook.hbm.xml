<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.NoteBook" table="note_book" schema="cloudnote">
        <id name="note_book_id" column="note_book_id"/>
        <property name="user_id" column="user_id"/>
        <property name="note_book_type_id"  column="note_book_type_id"/>
        <property name="note_book_name" column="note_book_name"/>
        <property name="explaination" column="explaination"/>
        <property name="created_at" column="created_at"/>
        <property name="updated_at" column="updated_at"/>
    </class>
</hibernate-mapping>